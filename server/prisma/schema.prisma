generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id               Int                 @id @default(autoincrement())
  email            String              @unique(map: "email") @db.VarChar(320)
  saved_businesses SavedYelpBusiness[] @relation("SavedYelpBusinessToUser")
}

model SavedYelpBusiness {
  id          Int    @id @default(autoincrement())
  business_id String @unique(map: "saved_business_id") @db.VarChar(50)
  users       User[] @relation("SavedYelpBusinessToUser")
}
